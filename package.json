{
  "name": "ctrl-enter",
  "displayName": "ctrl-Enter",
  "version": "0.0.3",
  "description": "'Enter'で送信するWebサイトを'Ctrl + Enter'に矯正するブラウザ拡張機能",
  "author": "INIAD.ts",
  "scripts": {
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:hcm": "hcm '**/*.module.css' -w --ignore-path .gitignore",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "",
    "lint:fix": "",
    "test": "plasmo test",
    "hcm": "hcm '**/*.module.css'",
    "typecheck": "pnpm hcm && tsc --noEmit"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.9.3",
    "@tabler/icons-react": "^2.47.0",
    "hook": "link:@plasmohq/storage/hook",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.243",
    "@types/node": "20.5.0",
    "@types/react": "18.2.75",
    "@types/react-dom": "18.2.24",
    "happy-css-modules": "^2.1.2",
    "less": "^4.2.0",
    "notios": "^0.4.1",
    "postcss": "^8.4.38",
    "sass": "^1.74.1",
    "typescript": "5.1.6"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
